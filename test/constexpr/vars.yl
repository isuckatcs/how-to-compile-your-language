// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn pass(): number {
    let x: number = 2.0;
    return x * 10.0;
}
// CHECK:     ReturnStmt
// CHECK-NEXT:       BinaryOperator: '*'
// CHECK-NEXT:       | value: 20
// CHECK-NEXT:         DeclRefExpr: @({{.*}}) x
// CHECK-NEXT:         NumberLiteral: '10'

fn pass2(): number {
    let x: number = 2.0;
    return x;
}
// CHECK:     ReturnStmt
// CHECK-NEXT:       DeclRefExpr: @({{.*}}) x
// CHECK-NEXT:       | value: 2

fn err(): number {
    let x: number;
    x = 2.0;
    return x * 10.0;
}
// CHECK:     ReturnStmt
// CHECK-NEXT:       BinaryOperator: '*'
// CHECK-NEXT:         DeclRefExpr: @({{.*}}) x
// CHECK-NEXT:         NumberLiteral: '10'

fn err2(): number {
    var x: number = 2.0;
    return x * 10.0;
}
// CHECK:    ReturnStmt
// CHECK-NEXT:      BinaryOperator: '*'
// CHECK-NEXT:        DeclRefExpr: @({{.*}}) x
// CHECK-NEXT:        NumberLiteral: '10'

fn main(): void {}
