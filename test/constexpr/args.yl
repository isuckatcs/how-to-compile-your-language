// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn foo(x: number): unit {}

fn bar(x: number): unit {
    foo(x);
}

fn main(): unit {
    bar(1.0);
}
// CHECK: FunctionDecl @({{.*}}) println {(number) -> unit}
// CHECK-NEXT:   ParamDecl @({{.*}}) n {number}
// CHECK-NEXT:   Block
// CHECK-NEXT: FunctionDecl @({{.*}}) foo {(number) -> unit}
// CHECK-NEXT:   ParamDecl @({{.*}}) x {number}
// CHECK-NEXT:   Block
// CHECK-NEXT: FunctionDecl @({{.*}}) bar {(number) -> unit}
// CHECK-NEXT:   ParamDecl @({{.*}}) x {number}
// CHECK-NEXT:   Block
// CHECK-NEXT:     CallExpr {unit}
// CHECK-NEXT:       DeclRefExpr @({{.*}}) foo {(number) -> unit}
// CHECK-NEXT:       DeclRefExpr @({{.*}}) x {number}
// CHECK-NEXT: FunctionDecl @({{.*}}) main {() -> unit}
// CHECK-NEXT:   Block
// CHECK-NEXT:     CallExpr {unit}
// CHECK-NEXT:       DeclRefExpr @({{.*}}) bar {(number) -> unit}
// CHECK-NEXT:       NumberLiteral '1' {number}
// CHECK-NEXT:       | value: 1
