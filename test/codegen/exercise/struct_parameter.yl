// RUN: compiler %s -llvm-dump 2>&1 | filecheck %s
// RUN: compiler %s -o struct_parameter && ./struct_parameter | grep -Plzx '1\n2\n'
struct S {
  x: number,
  y: number,
}

fn foo(s: S): unit {
  println(s.x);
  println(s.y);
}
// CHECK: define void @foo(ptr readonly %s) {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %0 = getelementptr inbounds nuw { double, double }, ptr %s, i32 0, i32 0
// CHECK-NEXT:   %1 = load double, ptr %0, align 8
// CHECK-NEXT:   call void @println(double %1)
// CHECK-NEXT:   %2 = getelementptr inbounds nuw { double, double }, ptr %s, i32 0, i32 1
// CHECK-NEXT:   %3 = load double, ptr %2, align 8
// CHECK-NEXT:   call void @println(double %3)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }

fn bar(mut s: S): S {
  return s;
}
// CHECK: define void @bar(ptr sret({ double, double }) %ret, ptr byval({ double, double }) %s) {
// CHECK-NEXT: entry:
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %ret, ptr align 8 %s, i64 16, i1 false)
// CHECK-NEXT:   br label %return
// CHECK-NEXT: 
// CHECK-NEXT: return:                                           ; preds = %entry
// CHECK-NEXT:   ret void
// CHECK-NEXT: }

fn main(): unit {
  mut s = S { x: 1, y: 2 };
  foo(s);
  s = bar(s);
}
// CHECK: define void @__builtin_main() {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %S.tmp = alloca { double, double }, align 8
// CHECK-NEXT:   %s = alloca { double, double }, align 8

// CHECK-NEXT:   %struct.ret.tmp = alloca { double, double }, align 8
// CHECK-NEXT:   %struct.arg.tmp = alloca { double, double }, align 8

// CHECK-NEXT:   %0 = getelementptr inbounds nuw { double, double }, ptr %S.tmp, i32 0, i32 0
// CHECK-NEXT:   store double 1.000000e+00, ptr %0, align 8
// CHECK-NEXT:   %1 = getelementptr inbounds nuw { double, double }, ptr %S.tmp, i32 0, i32 1
// CHECK-NEXT:   store double 2.000000e+00, ptr %1, align 8
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %s, ptr align 8 %S.tmp, i64 16, i1 false)

// CHECK-NEXT:   call void @foo(ptr readonly %s)

// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %struct.arg.tmp, ptr align 8 %s, i64 16, i1 false)
// CHECK-NEXT:   call void @bar(ptr sret({ double, double }) %struct.ret.tmp, ptr byval({ double, double }) %struct.arg.tmp)
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %s, ptr align 8 %struct.ret.tmp, i64 16, i1 false)

// CHECK-NEXT:   ret void
// CHECK-NEXT: }
