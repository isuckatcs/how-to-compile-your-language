// RUN: compiler %s -llvm-dump 2>&1 | filecheck %s
// RUN: compiler %s -o structs_generated_first && ./structs_generated_first | grep -Plzx '10\n120\n'

fn foo(s: S): S2 {
  return S2 { x: s.x, y: s.y };
}
// CHECK: define void @foo(ptr sret({ double, double }) %ret, ptr byval({ double, double }) %s) {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %S2.tmp = alloca { double, double }, align 8

// CHECK-NEXT:   %0 = getelementptr inbounds nuw { double, double }, ptr %s, i32 0, i32 0
// CHECK-NEXT:   %1 = load double, ptr %0, align 8
// CHECK-NEXT:   %2 = getelementptr inbounds nuw { double, double }, ptr %s, i32 0, i32 1
// CHECK-NEXT:   %3 = load double, ptr %2, align 8
// CHECK-NEXT:   %4 = getelementptr inbounds nuw { double, double }, ptr %S2.tmp, i32 0, i32 0
// CHECK-NEXT:   store double %1, ptr %4, align 8
// CHECK-NEXT:   %5 = getelementptr inbounds nuw { double, double }, ptr %S2.tmp, i32 0, i32 1
// CHECK-NEXT:   store double %3, ptr %5, align 8
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %ret, ptr align 8 %S2.tmp, i64 16, i1 false)

// CHECK-NEXT:   br label %return
// CHECK-NEXT: 
// CHECK-NEXT: return:                                           ; preds = %entry
// CHECK-NEXT:   ret void
// CHECK-NEXT: }

fn main(): unit {
  let x = S { x: 10, y: 120 };
  println(foo(x).x);
  println(foo(x).y);
}

struct S { x: number, y:number }
struct S2 { x: number, y:number }
