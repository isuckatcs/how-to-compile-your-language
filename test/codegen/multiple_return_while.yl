// RUN: compiler %s -llvm-dump 2>&1 | filecheck %s
// RUN: compiler %s -o multiple_return_while && ./multiple_return_while | grep -Plzx '0\n5\n3\n'
fn foo(mut x: number): number {
    mut n: number = x;
    while n > 10.0 {
        n = n - 1.0;
        
        if x == 15.0 {
            return 3.0;

            return 1.0;
            
            if 0.0 {

            } else {
                return 2.0;
            }
        }

        return 5.0;
        
        return 1.0;
        1.0 + 3.0;
    }

    return 0.0;
}
// CHECK: while.exit:                                       ; preds = %while.cond
// CHECK-NEXT:   store double 0.000000e+00, ptr %retval, align 8
// CHECK-NEXT:   br label %return
// CHECK-NEXT: 
// CHECK-NEXT: if.true:                                          ; preds = %while.body
// CHECK-NEXT:   store double 3.000000e+00, ptr %retval, align 8
// CHECK-NEXT:   br label %return
// CHECK-NEXT: 
// CHECK-NEXT: if.exit:                                          ; preds = %while.body
// CHECK-NEXT:   store double 5.000000e+00, ptr %retval, align 8
// CHECK-NEXT:   br label %return
// CHECK-NEXT: 
// CHECK-NEXT: return:                                           ; preds = %while.exit, %if.exit, %if.true
// CHECK-NEXT:   %7 = load double, ptr %retval, align 8
// CHECK-NEXT:   ret double %7

fn main(): void {
    println(foo(10.0));
    println(foo(11.0));
    println(foo(15.0));
}
