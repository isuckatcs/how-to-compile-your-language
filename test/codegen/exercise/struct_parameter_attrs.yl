// RUN: compiler %s -llvm-dump 2>&1 | filecheck %s
// RUN: compiler %s -o struct_parameter_attrs && ./struct_parameter_attrs | grep -Plzx '1\n2\n3\n2\n'
struct S {
  x: number,
}

fn foo(a: number, b: S, mut c: number, mut d: S): unit {
  println(a);
  println(b.x);
  println(c);
  println(d.x);
}
// CHECK:      define void @foo(double %a, ptr byval({ double }) %b, double %c, ptr byval({ double }) %d) {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %c1 = alloca double, align 8
// CHECK-NEXT:   store double %c, ptr %c1, align 8
// CHECK-NEXT:   call void @println(double %a)
// CHECK-NEXT:   %0 = getelementptr inbounds nuw { double }, ptr %b, i32 0, i32 0
// CHECK-NEXT:   %1 = load double, ptr %0, align 8
// CHECK-NEXT:   call void @println(double %1)
// CHECK-NEXT:   %2 = load double, ptr %c1, align 8
// CHECK-NEXT:   call void @println(double %2)
// CHECK-NEXT:   %3 = getelementptr inbounds nuw { double }, ptr %d, i32 0, i32 0
// CHECK-NEXT:   %4 = load double, ptr %3, align 8
// CHECK-NEXT:   call void @println(double %4)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }

fn main(): unit {
  let s = S { x: 2 };
  foo(1, s, 3, s);
}
// CHECK:      define void @__builtin_main() {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %S.tmp = alloca { double }, align 8
// CHECK-NEXT:   %s = alloca { double }, align 8
// CHECK-NEXT:   %struct.arg.tmp = alloca { double }, align 8
// CHECK-NEXT:   %struct.arg.tmp1 = alloca { double }, align 8
// CHECK-NEXT:   %0 = getelementptr inbounds nuw { double }, ptr %S.tmp, i32 0, i32 0
// CHECK-NEXT:   store double 2.000000e+00, ptr %0, align 8
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %s, ptr align 8 %S.tmp, i64 8, i1 false)
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %struct.arg.tmp, ptr align 8 %s, i64 8, i1 false)
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %struct.arg.tmp1, ptr align 8 %s, i64 8, i1 false)
// CHECK-NEXT:   call void @foo(double 1.000000e+00, ptr byval({ double }) %struct.arg.tmp, double 3.000000e+00, ptr byval({ double }) %struct.arg.tmp1)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }
