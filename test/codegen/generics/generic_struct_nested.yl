// RUN: compiler %s -llvm-dump 2>&1 | filecheck %s
// RUN: compiler %s -o generic_struct_nested && ./generic_struct_nested | grep -Plzx '10\n'

struct A<T> {
  x: T
}

struct B<T> {
  a: A<T>
}

fn main(): void {
  let b = B{a: A{x: 10}};
  println(b.a.x);
}
// CHECK: define void @__builtin_main() {
// CHECK-NEXT: entry:
// CHECK-NEXT:   %b = alloca { { double } }, align 8
// CHECK-NEXT:   %B.tmp = alloca { { double } }, align 8
// CHECK-NEXT:   %A.tmp = alloca { double }, align 8

// CHECK-NEXT:   %0 = getelementptr inbounds nuw { { double } }, ptr %B.tmp, i32 0, i32 0
// CHECK-NEXT:   %1 = getelementptr inbounds nuw { double }, ptr %A.tmp, i32 0, i32 0
// CHECK-NEXT:   store double 1.000000e+01, ptr %1, align 8
// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %0, ptr align 8 %A.tmp, i64 8, i1 false)

// CHECK-NEXT:   call void @llvm.memcpy.p0.p0.i64(ptr align 8 %b, ptr align 8 %B.tmp, i64 8, i1 false)
// CHECK-NEXT:   %2 = getelementptr inbounds nuw { { double } }, ptr %b, i32 0, i32 0
// CHECK-NEXT:   %3 = getelementptr inbounds nuw { double }, ptr %2, i32 0, i32 0
// CHECK-NEXT:   %4 = load double, ptr %3, align 8
// CHECK-NEXT:   call void @println(double %4)
// CHECK-NEXT:   ret void
// CHECK-NEXT: }
