// RUN: compiler %s -res-dump 2>&1 | filecheck %s --match-full-lines
fn foo(x: number): void {}

fn bar(x: number): void {
    foo(x);
}

fn main(): void {
    bar(1.0);
}

// CHECK: FunctionDecl: @({{.*}}) foo:
// CHECK-NEXT:   ParamDecl: @({{.*}}) x:
// CHECK-NEXT:   Block
// CHECK-NEXT: FunctionDecl: @({{.*}}) bar:
// CHECK-NEXT:   ParamDecl: @({{.*}}) x:
// CHECK-NEXT:   Block
// CHECK-NEXT:     CallExpr:
// CHECK-NEXT:       DeclRefExpr: @({{.*}}) foo
// CHECK-NEXT:       DeclRefExpr: @({{.*}}) x
// CHECK-NEXT: FunctionDecl: @({{.*}}) main:
// CHECK-NEXT:   Block
// CHECK-NEXT:     CallExpr:
// CHECK-NEXT:       DeclRefExpr: @({{.*}}) bar
// CHECK-NEXT:       NumberLiteral: '1'
// CHECK-NEXT:       | value: 1
