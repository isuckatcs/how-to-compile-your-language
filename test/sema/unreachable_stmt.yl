// RUN: compiler %s 2>&1 | filecheck %s
fn singleReturn(): unit {
    return;
}

fn multipleReturns(): unit {
    1.0;
    return;

    // CHECK: [[# @LINE + 1 ]]:5: warning: unreachable statement
    2.0;
    return;
    
    3.0;
    return;
}

fn multipleReturnsAfterEachOther(): unit {
    return;
    // CHECK: [[# @LINE + 1 ]]:5: warning: unreachable statement
    return;
}

fn branch(x: number): unit {
    if 1.0 || x {
        1.0;
        return;
        // CHECK: [[# @LINE + 1 ]]:9: warning: unreachable statement
        2.0;
    }

    return;
    // CHECK: [[# @LINE + 1 ]]:9: warning: unreachable statement
    3.0 + 5.0;
}

fn loop(x: number): unit {
    while x {
        return;
        // CHECK: [[# @LINE + 1 ]]:9: warning: unreachable statement
        1.0;
    }
}

fn main(): unit {}
