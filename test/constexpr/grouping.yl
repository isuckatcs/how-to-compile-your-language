// RUN: compiler %s -res-dump 2>&1 | filecheck %s
fn foo(): number {
    let x: number = 2.1;
    let y: number = 5.3;

    return (10.0 * (x + 4.0)) && (!(y == x) || x < y);
}
// CHECK: ReturnStmt
// CHECK-NEXT:   BinaryOperator '&&' {number}
// CHECK-NEXT:   | value: 1

