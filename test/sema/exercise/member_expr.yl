// CHECK-NEXT: FunctionDecl: @({{.*}}) main:
// CHECK-NEXT:   Block
// CHECK-NEXT:     DeclStmt:
// CHECK-NEXT:       VarDecl: @({{.*}}) s:
// CHECK-NEXT:         StructInstantiationExpr: @({{.*}})
// CHECK-NEXT:           FieldInitStmt: @({{.*}}) x
